# Environment Variables for Le Havre Aixois
# Copy this file to .env.local and fill in your actual values

# Google Analytics 4 Measurement ID
# Get this from https://analytics.google.com
# Format: G-XXXXXXXXXX
NEXT_PUBLIC_GA_MEASUREMENT_ID=

# Contact Form Configuration
# Your contact email for inquiries
NEXT_PUBLIC_CONTACT_EMAIL=contact@lehavreaixois.com

# Contact form endpoint (Google Apps Script or other webhook)
# Leave empty if using mailto: links
NEXT_PUBLIC_CONTACT_ENDPOINT=
